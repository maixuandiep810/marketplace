<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>@ViewData["Title"]</title>

  @RenderSection("Styles", required: false)

  <partial name="AdminLTE/_Styles" />
</head>




<body class="hold-transition sidebar-mini">


  <div class="wrapper">


    <partial name="AdminLTE/_TopNavigation" />
    <partial name="AdminLTE/_MainNavigation" />


    <div class="content-wrapper">
        
          
      <partial name="AdminLTE/_AlertContent"/>
      <div class="content">
        <div class="container-fluid">
          @RenderBody()
        </div>
      </div>
    </div>

    @* <aside class="control-sidebar control-sidebar-dark">
      <div class="p-3">
      <h5>Title</h5>
      <p>Sidebar content</p>
      </div>
      </aside> *@


      


    <partial name="AdminLTE/_Footer" />
    <partial name="AdminLTE/_AlertContent" />

  </div>


  <partial name="AdminLTE/_Scripts" />

  @RenderSection("RenderSectionScripts", required: false)
</body>

</html>