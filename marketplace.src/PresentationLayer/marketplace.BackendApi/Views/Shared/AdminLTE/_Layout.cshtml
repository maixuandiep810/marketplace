<!DOCTYPE html>
<html lang="en">




<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>@ViewData["Title"]</title>

  <partial name="~/Views/Shared/AdminLTE/_Styles.cshtml" />

  @RenderSection("MPStyles", required: false)

</head>












<body class="hold-transition sidebar-mini layout-fixed">


  <div class="wrapper">

    <partial name="~/Views/Shared/AdminLTE/_Preloader.cshtml" />

    @RenderSection("TopNavigation", required: false)

    @RenderSection("MainNavigation", required: false)

    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">

      @RenderSection("ContentHeader", required: false)

      <!-- Main content -->
      <section class="content">
        <div class="container-fluid">
          @RenderBody()
        </div>
      </section>
      <!-- /.content -->

    </div>
    <!-- /.content-wrapper -->

    <partial name="~/Views/Shared/AdminLTE/_Footer.cshtml" />

  </div>
  <!-- ./wrapper -->


  <partial name="~/Views/Shared/AdminLTE/_Scripts.cshtml" />

  @RenderSection("MPScripts", required: false)






</body>

</html>